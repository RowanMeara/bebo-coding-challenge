extends layout

block content
  h1= title
  p Welcome to #{title}
  video(id='local-video' autoplay muted width='640' height='480')
  video(id='remote-video' width='640' height='480' controls)
  h2= 'Microphone Volume'
  input(type='range' id='micvolume')
  input(type='button' id='start-button' value='Start Call')